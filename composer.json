{
    "name": "litepie/masters",
    "description": "Laravel package for managing master data with multi-tenancy support",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "master-data",
        "multi-tenancy",
        "countries",
        "states",
        "cities",
        "categories"
    ],
    "authors": [
        {
            "name": "Litepie Team",
            "email": "support@litepie.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-permission": "^6.0",
        "spatie/laravel-multitenancy": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Litepie\\Masters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Litepie\\Masters\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Litepie\\Masters\\MastersServiceProvider"
            ],
            "aliases": {
                "Masters": "Litepie\\Masters\\Facades\\Masters"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
